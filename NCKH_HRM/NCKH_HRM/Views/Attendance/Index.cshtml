@model IEnumerable<NCKH_HRM.Models.Term>
@{
    ViewData["Title"] = "Home Page";
}
<h1>Danh sách các học phần</h1>
<div class="list-group">
 
    @foreach (var item in Model)
    {
        <a asp-action="" asp-route-id="@item.Id" class="list-group-item list-group-item-action">@item.Name</a>
    }

</div>
